{
  "name": "Tugure",
  "version": "1.0.0",
  "description": "This is an Online Market Platform where Buyers get chance to buy cheap and trusted items from markets and shops across the country",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shemaeric/Tugure.git"
  },
  "keywords": [],
  "author": "Anonymous",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shemaeric/Tugure/issues"
  },
  "homepage": "https://github.com/shemaeric/Tugure#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ./index.js --exec babel-node",
    "start:heroku": "babel-node index.js",
    "heroku-postbuild": "npm install",
    "migration:generate": "node_modules/.bin/sequelize migrations:generate --name",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "migrate:refresh": "npm run migrate:reset && npm run migrate",
    "migrate:reset": "node_modules/.bin/sequelize db:migrate:undo:all",
    "test:all": "NODE_ENV=test npm run migrate:refresh && npm run seed && npm test"
  },
  "engines": {
    "node": "11.10.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@sendgrid/mail": "^6.3.1",
